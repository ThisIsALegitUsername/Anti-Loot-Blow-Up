{
	"schemaVersion": 1,
	"id": "anti-loot-blow-up",
	"version": "${version}",
	"name": "Anti Loot Blow Up",
	"description": "Disallows you from breaking crystals when a dead entity is nearby.",
	"authors": [
		"hooman"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/anti-loot-blow-up/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"dev.hooman.AntiLootBlowUp"
		],
		"client": [
			"dev.hooman.AntiLootBlowUpClient"
		]
	},
	"mixins": [
		"anti-loot-blow-up.mixins.json",
		{
			"config": "anti-loot-blow-up.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.15.11",
		"minecraft": "~1.20.1",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
